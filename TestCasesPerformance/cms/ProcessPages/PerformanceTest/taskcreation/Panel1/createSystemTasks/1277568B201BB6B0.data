<FORM NAME="formcreateSystemTasks" METHOD="POST" ACTION='<%=ivy.html.ref("createTasks.ivp")%>'  >
<!--ivyform--><table BORDER="0"><tr >
<td COLSPAN="2"><!--t--><P>
<B><FONT size=4>Case and task generator</B></FONT>
</P></td>
</tr>
<tr >
<td ><!--i.tcraete.repeat-->Repeat</td>
<td ><!--i.acraete.repeat--><%=ivy.html.checkbox("in.craete.repeat","craete$repeat","")%> <INPUT TYPE="hidden" NAME="craete$repeat" VALUE="false"></td>
</tr>
<tr >
<td ><!--i.tcraete.runningSec-->Running seconds</td>
<td ><!--i.acraete.runningSec--><INPUT TYPE="TEXT" NAME="craete$runningSec"  VALUE="<%=ivy.html.get("in.craete.runningSec")%>"></td>
</tr>
<tr >
<td ><!--i.tcraete.countOfTasks-->Count of tasks for each case</td>
<td ><!--i.acraete.countOfTasks--><INPUT TYPE="TEXT" NAME="craete$countOfTasks"  VALUE="<%=ivy.html.get("in.craete.countOfTasks")%>"></td>
</tr>
<tr >
<td ><!--i.tcraete.sqlQuery-->Executed SQL query (could be empty)</td>
<td ><!--i.acraete.sqlQuery--><TEXTAREA NAME="craete$sqlQuery"  ROWS="5" COLS="60"><%=ivy.html.getObject("in.craete.sqlQuery")%></TEXTAREA></td>
</tr>
<tr >
<td ></td>
<td ><!--i.submit-->
<INPUT  TYPE="SUBMIT" NAME="Submit" VALUE="start"></td>
</tr>


</table></FORM>